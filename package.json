{
  "name": "nbg-business-insights",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run mock-server\" \"vite\"",
    "dev:app-only": "vite",
    "dev:api-only": "cd mock-server && npm run dev",
    "mock-server": "cd mock-server && npm run dev",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && npm run lint",
    "preview": "vite preview",
    "setup:mock-server": "cd mock-server && npm install",
    "test:mock-server": "cd mock-server && npm test",
    "proxy": "npm run build && cd proxy-server/node && npm start",
    "proxy:dev": "cd proxy-server/node && npm run dev",
    "proxy:setup": "cd proxy-server/node && npm install && npm run generate-certs",
    "proxy:test": "cd proxy-server/node && npm test",
    "proxy:node": "npm run build && cd proxy-server/node && npm start",
    "proxy:csharp": "npm run build && cd proxy-server/csharp/MerchantInsightsProxy && dotnet run"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "date-fns": "^4.1.0",
    "i18next": "^23.15.0",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-datepicker": "^7.3.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.26.0",
    "react-select": "^5.8.0",
    "recharts": "^2.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "vite": "^6.3.5"
  }
}
